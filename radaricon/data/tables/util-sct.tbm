#Conditional Hooks
$Application: FS2_Open
$On Game Init:
[

function removeFromTable(src, indexList, maxNr)
	
	if #indexList == 0 then
		return
	end

	for i = 1, #indexList, 1 do
		src[indexList[i]] = nil
	end
	
	local nextFree = 0

	for i = 1, maxNr, 1 do
		if src[i] == nil then
			if nextFree == 0 then
				nextFree = i
			end
		else
			if nextFree > 0 then
				src[nextFree] = src[i]
				src[i] = nil
				nextFree = nextFree + 1
			end
		end
	end
end

]
#End